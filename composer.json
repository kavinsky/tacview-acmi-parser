{
    "name": "kavinsky/tacview-acmi-parser",
    "description": "A PHP8 TacView ACMI file format reader",
    "keywords": [
        "kavinsky",
        "tacview-acmi-reader"
    ],
    "homepage": "https://github.com/kavinsky/tacview-acmi-reader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ignacio Munoz Fernandez",
            "email": "nmunozfernandez@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.64",
        "marc-mabe/php-enum": "^4.4"
    },
    "suggest": {
        "ext-zip": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "pestphp/pest-plugin-mock": "^1.0",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Kavinsky\\TacviewAcmiParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kavinsky\\TacviewAcmiReader\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
